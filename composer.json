{
    "name": "drupal/gdpr",
    "description": "Helps with making a site GDPR-compliant.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "mhavelant",
            "email": "mhavelant@brainsum.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "ext-zip": "*",
        "drupal/checklistapi": "^1.0",
        "drupal/entity": "^1.0",
        "drupal/entity_reference_revisions": "^1.4",
        "drupal/message": "^1.0",
        "drupal/token": "^1.1",
        "fzaninotto/faker": "^1.7"
    },
    "suggest": {
        "drush/drush": "^9 || ^10"
    },
    "extra": {
        "drush": {
            "services": {
                "modules/gdpr_dump/drush.services.yml": "^9 || ^10"
            }
        }
    }
}
